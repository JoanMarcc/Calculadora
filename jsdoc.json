{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["script.js"],
        "exclude": ["node_modules"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "destination": "./docs",
        "recurse": true
    }
}